{
    "name":  "erdnaxelaweb/contentadditionalinformations",
    "type": "ibexa-bundle",
    "description": "Add a service to manage content related additional informations in the DB.",
    "keywords": [
        "ibexa",
        "symfony"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ibexa/core": "^4.0",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "symplify/easy-coding-standard": "*",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-symfony": "^2.0"
    },
    "suggest": {
        "ext-dom": "*"
    },
    "authors": [
        {
            "name": "Florian ALEXANDRE"
        }
    ],
    "scripts": {
        "check-cs": [
            "ecs"
        ],
        "fix-cs": [
            "ecs --fix"
        ]
    },
    "autoload": {
        "psr-4": {
            "ErdnaxelaWeb\\ContentAdditionalInformationsBundle\\": "bundle",
            "ErdnaxelaWeb\\ContentAdditionalInformations\\": "lib",
            "ErdnaxelaWeb\\ContentAdditionalInformations\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "ibexa/post-install": true,
            "symfony/flex": true,
            "phpstan/extension-installer": true
        }
    },
    "repositories": {
        "ibexa": {
            "type": "composer",
            "url": "https://updates.ibexa.co"
        }
    }
}
